#include <iostream>
using namespace std;
struct node
{
    int data;
    struct node *left;
    struct node *right;
};
void Insert(struct node *root,int data)
{
    struct node *ptr=(struct node*)malloc(sizeof(struct node));
    ptr->data=data;
    ptr->left=NULL;
    ptr->right=NULL;
    struct node *prev;
    while(root!=NULL)
    {
        prev=root;
        if(root->data==data)
        {
            return;
        }
        else if(root->data>data)
        {
            root=root->left;
        }
        else
        {
            root=root->right;
        }
    }
    if(prev->data>data)
    {
        prev->left=ptr;
    }
    else
    {
        prev->right=ptr;
    }
}
static int c=0;
void Node_With_No_Child(struct node *root)
{
    if(root!=NULL)
    {
        Node_With_No_Child(root->left);
        if(root->left==NULL && root->right==NULL)
        {
            c++;
        }
        Node_With_No_Child(root->right);
    }
}
int main()
{
    struct node *root=(struct node *)malloc(sizeof(struct node));
    root->data=6;
    root->left=NULL;
    root->right=NULL;
    Insert(root,4);
    Insert(root,8);
    Insert(root,5);
    Insert(root,7);
    Insert(root,12);
    Insert(root,2);
    Node_With_No_Child(root);
    cout<<c;
    return 0;
}
